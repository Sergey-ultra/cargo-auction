            <link href="{{ asset('assets/css/notify.css') }}" rel="stylesheet"/>
            <link href="{{ asset('assets/css/app.css') }}" rel="stylesheet"/>
        {% endblock %}
        <script>
            window.locale = '{{ app.request.locale }}';
            window.yandexMapApikey = '{{ yandexMapApikey }}'
            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                window.authData = {
                    userId: {{ app.user.id }},
                    email: '{{ app.user.email }}'
                };
            {% endif %}
        </script>


        <script type="text/javascript" src="{{ asset('assets/js/notify.js') }}"></script>

